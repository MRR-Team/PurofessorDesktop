<Window x:Class="Purofessor.GuestWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:components="clr-namespace:Purofessor.components"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Purofessor"
        mc:Ignorable="d"
        Title="Purofessor"
        Height="750" Width="1000"
        MinWidth="640" MinHeight="640"
        Background="{DynamicResource BackgroundBrush}"
        >
    <Grid>
        <components:TopPanel_Guest HorizontalAlignment="Stretch" VerticalAlignment="Top"/>

        <Frame x:Name="GuestFrame" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>

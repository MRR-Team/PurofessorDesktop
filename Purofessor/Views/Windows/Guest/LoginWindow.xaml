<Window x:Class="Purofessor.Views.Windows.Guest.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Purofessor"
        mc:Ignorable="d"
        Title="Witamy w Purofessor"
        Height="600" Width="450"
        MinWidth="450" MinHeight="600">

    <Window.Background>
        <LinearGradientBrush StartPoint="0,0" EndPoint="1,1">
            <GradientStop Color="{DynamicResource GradientStartColor}" Offset="0"/>
            <GradientStop Color="{DynamicResource GradientEndColor}" Offset="1"/>
        </LinearGradientBrush>
    </Window.Background>

    <Grid>
        <Frame x:Name="LoginFrame" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>

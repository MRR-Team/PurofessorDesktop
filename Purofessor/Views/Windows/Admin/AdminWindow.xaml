<Window x:Class="Purofessor.Views.Windows.Admin.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:components="clr-namespace:Purofessor.components"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Purofessor"
        mc:Ignorable="d"
        Height="650" Width="800"
        MinWidth="800" MinHeight="650"
        Background="{DynamicResource MainBackgroundGradient}"
        Title="AdminWindow">
    <Window.Resources>
        <LinearGradientBrush x:Key="MainBackgroundGradient" StartPoint="0,0" EndPoint="1,1">
            <GradientStop Color="{DynamicResource WindowGradientStartColor}" Offset="0" />
            <GradientStop Color="{DynamicResource WindowGradientStartColor}" Offset="1" />
        </LinearGradientBrush>
    </Window.Resources>

    <Grid>
        <components:TopPanel_Admin HorizontalAlignment="Stretch" VerticalAlignment="Top"/>
        <Frame x:Name="MainFrame" NavigationUIVisibility="Hidden"/>


    </Grid>
</Window>
